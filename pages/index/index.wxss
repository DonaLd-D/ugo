.banners{
  height: 340rpx;
}
.banners image{
  height: 340rpx;
  width: 750rpx;
}

.menus{
    display: flex;
    justify-content: space-around;
    align-items: center;
    padding:20rpx 0;
}

.menus image{
    width:128rpx;
    height:140rpx;
}

.floor{
  
  height: 470rpx;
  margin-top:5px;
  position: relative;
}
.floor >image{
  background: #eee;
  width: 100%;
  height: 60rpx;
}
.floor-left image{
  width: 232rpx;
  height: 386rpx;
  padding: 4px;
}
.floor-right{
  position: absolute;
  top:70rpx;
  left: 250rpx;
  height: 386rpx;
  display: flex;
  flex-wrap: wrap;
}
.floor-right image{
  width: 233rpx;
  height: 184rpx;
  padding: 4px;
}
.floor+.floor .floor-right image{
  width: 193rpx;
}
.floor+.floor .floor-right image:nth-child(3n+1){
  width: 273rpx;
}
