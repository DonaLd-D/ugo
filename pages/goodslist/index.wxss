.search{
  background: #eee;
  padding: 20rpx 16rpx;
}
.searchinput{
  background: #fff;
  height: 60rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  border-radius: 4px;
  font-size: 12px;
}

.tab{
  height: 100rpx;
  border-bottom: 1px solid #eee;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 14px;
}
.active{
  color:#ff2d4a;
}

.goodslist navigator{
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
  border-bottom: 1px solid #eee;
}
.goodsleft{
  height: 200rpx;
  width: 200rpx;
  flex-shrink: 0;
}
.goodsright{
  flex: 1;
  display: flex;
  padding-left: 20rpx;
  flex-direction: column;
  justify-content: space-between;
  font-size: 14px;
}
.goodsright text{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.goodsright view{
  color:#ff2d4a;
}

.tips{
  font-size: 14px;
  text-align: center;
  padding: 20rpx;
  color: #888;
}