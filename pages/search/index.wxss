.searchinput{
  background: #eee;
  height: 60rpx;
  width: 750rpx;
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
}
.searchleft{
  flex: 1;
  background: #fff;
  display: flex;
  padding: 5px;
  align-items: center;
  font-size: 14px;
}
.searchright{
  border: 1px solid #aaa;
  padding: 3px 18px;
  margin: 2px 50rpx;
  align-self: center;
  font-size: 14px;
  border-radius: 4px;
}

.historysearch{
  display: flex;
  justify-content: space-between;
  padding: 20rpx 30rpx;
}

.historycontent{
  padding: 10rpx 30rpx;
  display: flex;
  justify-content: flex-start;
}
.historycontent navigator{
  background: #eee;
  padding: 20rpx;
  margin-right: 40rpx;
  font-size: 13px;
}